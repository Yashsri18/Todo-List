<%- include("header");-%>
   <div class="box" id="heading">
       <h1><%= listTitle %></h1>

   </div>
   <div class="box">
    <%for(var i = 0; i < newListItem.length; i++){%>
        <div class="item">
            <input type="checkbox" name="" id="">
            <p><%= newListItem[i] %></p>
        </div>
        <%}%>
        
        <form action="/" method="post">
            <input type="text" name="newItem" autofocus placeholder="New Task" autocomplete="off">
            <button type="submit"name="list" value=<%= listTitle %>>+</button>
   </div>

</form>
<%- include("footer");-%>
